  "scripts": {
    "::==================[ COMMON ]": "#==================",
    "start": "yarn dev:init",
    "postinstall": "typings install",
    "gen:stats": "yarn prod:client -- --profile --display-modules --json > stats.json",
    "::==================[ DEVELOPMENT ]": "#==================",
    "dev:init": "yarn && yarn dev:composer && yarn build:all",
    "dev:composer": "composer install  && yarn install --force && if [ -z ${CDN+x} ]; then echo '\\033[0;103m\\033[1;91mNotice: CDN not given, dont forget to link it!\\033[0m' ; else ln -Fs /Volumes/cdn/${CDN} fileadmin; fi",
    "dev:all": "SESSION=$USER; tmux -2 new-session -d -s $SESSION; tmux rename-window -t $SESSION:0 \"Dev\"; tmux split-window  -h; tmux select-pane -t 0; tmux send-keys \"yarn dev:client\" C-m; tmux select-pane -t 1; tmux send-keys \"yarn dev:server\" C-m; tmux select-window -t $SESSION:0; tmux -2 attach-session -t $SESSION",
    "dev:server": "PORT=8420; cross-env DASHBOARD_PORT=$PORT webpack-dashboard --port $PORT --color red -- webpack --mode=development --config='webpack.config.server.js' --watch",
    "dev:client": "webpack-dashboard --color blue -- webpack --mode=development --config='webpack.config.client.js' --watch",
    "::==================[ BUILD ]": "#==================",
    "build:all": "yarn build:client && yarn build:server",
    "build:server": "webpack --mode=development --config='webpack.config.server.js' --progress",
    "build:client": "webpack --mode=development --config='webpack.config.client.js' --progress",
    "::==================[ PRODUCTION ]": "#==================",
    "prod:deploy": "cross-env NODE_ENV=production  yarn prod:composer && yarn && yarn prod:all",
    "prod:all": "cross-env NODE_ENV=production     yarn prod:client && yarn prod:server",
    "prod:server": "cross-env NODE_ENV=production  webpack --mode=production --config='webpack.config.server.js' --display-modules",
    "prod:client": "cross-env NODE_ENV=production  webpack --mode=production --config='webpack.config.client.js' --display-modules",
    "prod:composer": "composer install --no-ansi --no-dev --no-interaction --no-progress --optimize-autoloader"
  },
